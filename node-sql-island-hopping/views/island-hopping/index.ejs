<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
    <title>JAC Enterprises Island Hopping</title>
  </head>

  <body>
    <%- include('../partials/header'); %>

    <h1>JAC Enterprises Island Hopping</h1>
    <% if (messages.success) { %>
    <div class="alert alert-success" role="alert"><%- messages.success %></div>
    <% } %> <% if (messages.error) { %>
    <div class="alert alert-danger" role="alert"><%- messages.error %></div>
    <% } %>

    <h2>
      <% if (data.length) { %>
      <table>
        <tbody>
          <% for(let i = 0; i < data.length; i++) { %>
          <tr>
            <td>
              <a href="./island-hopping/continent/<%= data[i].continent_id%>"
                ><%= data[i].location%></a
              >
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
      <% } %>
    </h2>
    <%- include('../partials/footer'); %>
  </body>
</html>
